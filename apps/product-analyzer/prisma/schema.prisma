// Try Prisma Accelerate: https://pris.ly/cli/accelerate-init

generator client {
  provider = "prisma-client-js"
  output   = "./generated/client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Additive {
  id       String @id @default(autoincrement())
  name     String @unique()
  info     String
  origin   String
  category String
  danger   Int
  benefits String
  harm     String
}
